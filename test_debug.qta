start default

  define application "Domestic Refrigeration"
  
    uses substance "HFC-134a"
      enable domestic
      initial charge with 0.15 kg / unit for domestic
      equals 1430 kgCO2e / unit
      set domestic to 25 mt / year during year 2025
    end substance
  
  end application

end default

start simulations
  simulate "BAU" from years 2025 to 2026
end simulations