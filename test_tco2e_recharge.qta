start default

  define application "test_app"
    uses substance "test_substance"
      enable domestic
      initial charge with 0.15 kg / unit for domestic
      equals 1.43 tCO2e / unit
      set domestic to 25 mt during year 1
      recharge 10 % with 0.15 kg / unit
    end substance
  end application

end default

start simulations
  simulate "test" from years 1 to 3
end simulations