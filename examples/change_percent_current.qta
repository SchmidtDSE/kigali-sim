start default

  define application "test"

    uses substance "test"
      enable domestic
      initial charge with 1 kg / unit for domestic
      set domestic to 100 kg during year 1
      change domestic by +10 % current each year during years 2 to 3
      equals 5 tCO2e / mt
    end substance

  end application

end default


start simulations

  simulate "business as usual" from years 1 to 3

end simulations
