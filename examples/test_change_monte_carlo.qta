start default

  define application "test"

    uses substance "test"
      enable domestic
      set domestic to 100 units / year during year 2025
      change equipment by sample normally from mean of 6 std of 1 % / year during years 2025 to 2030
      equals 5 tCO2e / unit
    end substance

  end application

end default


start simulations

  simulate "test" from years 2025 to 2026 across 10 trials

end simulations