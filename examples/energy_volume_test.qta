start default

  define application "Test"
  
    uses substance "SubA"
      enable domestic
      initial charge with 1 kg / unit for domestic
      equals 1 tCO2e / kg
      equals 0.5 kwh / kg
      set sales to 1 mt during year 1
      recharge 5 % with 0.85 kg / unit
    end substance
  
  end application

end default


start simulations

  simulate "BAU"
  from years 1 to 10

end simulations