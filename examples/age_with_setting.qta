start default
  define application "test"
    uses substance "test"
      enable domestic
      initial charge with 1 kg / unit for domestic
      set priorEquipment to 500 units during year 1
      set age to 10 years during year 1
      set domestic to 50 units during years 2 to 3
      retire 5 %
      equals 5 tCO2e / mt
    end substance
  end application
end default

start simulations
  simulate "business as usual" from years 1 to 3
end simulations
